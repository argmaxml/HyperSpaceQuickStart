      ],
      "source": [
        "from pprint import pprint\n",
        "response = hyperspace_client.set_function('/content/drive/MyDrive/Data/ArXiv/score_func.py', collection_name=collection_name, function_name='similarity_sf')\n",
        "\n",
        "query_with_knn = {\n",
        "    'vector_Content': input_vector,\n",
